{
  "name": "the-zen-escape",
  "version": "1.0.0",
  "description": "Learning NodeJs, Express and MongoDB",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "star:dev": "npx forever -w server.js",
    "start-node:prod": "SET NODE_ENV=production&&node server.js",
    "start:prod": "SET NODE_ENV=production&&npx forever -w server.js",
    "debug": "ndb server.js",
    "debug:prod": "SET NODE_ENV=production&&ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Vedant Shinde",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "forever": "^3.0.2",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.4.15",
    "pug": "^3.0.0",
    "sharp": "^0.26.3",
    "slugify": "^1.4.5",
    "stripe": "^7.0.0",
    "validator": "^13.1.17",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": "^14"
  }
